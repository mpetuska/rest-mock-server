{
  "name": "rest-mock-server",
  "version": "1.0.0",
  "description": "Remotely configurable mock REST API server",
  "main": "src/index.ts",
  "repository": "https://github.com/mpetuska/rest-mock-server.git",
  "author": "mpetuska <martynas.petuska@outlook.com>",
  "license": "Apache-2",
  "private": true,
  "scripts": {
    "start": "nodemon --files src/index.ts",
    "build": "webpack",
    "vscode": "yarn dlx @yarnpkg/pnpify --sdk vscode"
  },
  "dependencies": {
    "application.env": "^1.0.17",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-promise-router": "^4.0.1",
    "joi": "^17.4.0",
    "source-map-support": "^0.5.19"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.11",
    "@types/node": "^18.0.0",
    "@types/prettier": "^2.2.3",
    "@types/source-map-support": "^0.5.3",
    "@types/webpack": "^4.41.26",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "eslint": "^7.22.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fork-ts-checker-webpack-plugin": "^6.1.1",
    "husky": "^4.3.7",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0"
  },
  "nodemonConfig": {
    "ext": "*.ts",
    "watch": [
      "src"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn dedupe && yarn stage",
      "post-commit": "git add -u; git commit --amend -C HEAD --no-verify; fi;"
    }
  }
}
